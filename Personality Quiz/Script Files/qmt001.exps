    macro AddPtsForTD($nature_0, $nature_1, $nature_2, $nature_3, $nature_4, $nature_5, $nature_6, $nature_7, $nature_8, $nature_9, $nature_10, $nature_11, $nature_12, $nature_13, $nature_14, $nature_15, $nature_16, $nature_17, $nature_18, $nature_19, $nature_20, $nature_21, $nature_22, $nature_23, $nature_24, $nature_25, $nature_26, $nature_27, $nature_28, $nature_29, $nature_30, $nature_31, $nature_32, $nature_33, $nature_34, $nature_35, $nature_36, $nature_37, $nature_38, $nature_39, $nature_40, $nature_41, $nature_42, $nature_43, $nature_44, $nature_45, $nature_46, $nature_47, $nature_48, $nature_49, $nature_50) {
