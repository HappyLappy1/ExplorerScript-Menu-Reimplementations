    macro CompareNaturePts($var,$index) {
        if ($var == value($SCENARIO_SUB2) ) {
            $LOCAL1 = $index;
            $SCENARIO_MAIN += 1;
            switch(random(2)) {
               case 1:
                   $var+= 1;
               default:
                   break;
             }
        }
    }
