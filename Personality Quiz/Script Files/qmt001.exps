	    $nature_10 += 4; // Relaxed Nature gets points if you have played T/D before, regardless of gender!
	    ~SumToLocal($nature_10);
	    if($BIT_FUWARANTE_LOCAL[4]) { // Player chose "Male"
	        $nature_7 += 4; // Hasty Nature gets points if you have played T/D before and are male!
	        ~SumToLocal($nature_7);
	        $nature_8 += 4; // Sassy Nature gets points if you have played T/D before and are male!
