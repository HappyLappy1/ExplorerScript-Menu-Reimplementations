            } elseif ($BIT_FUWARANTE_LOCAL[3]) { // Player chose "Female"
	        $nature_3 += 4; // Jolly Nature gets points if you have played T/D before and are male!
	        ~SumToLocal($nature_3);                
            } else { // Player chose "Surprise Me"
	        // Add pts to all of them for surprise!
	        $nature_7 += 4;
	        ~SumToLocal($nature_7);
	        $nature_8 += 4;
	        ~SumToLocal($nature_8);
	        $nature_3 += 4;
	        ~SumToLocal($nature_3); 
	    }
	
        }
    }

	
	
	
    // Note: This just keeps track of how many points the winning nature has. It's super important that we know this to make coding the tally syste easier...
