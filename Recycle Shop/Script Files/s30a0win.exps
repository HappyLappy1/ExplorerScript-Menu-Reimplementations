            message_Mail({
                english="[CN]The [s_item:0] was\n[CN]sent to storage.",
            });
            CallCommon(CORO_MESSAGE_CLOSE_WAIT_FUNC);
            switch ( ProcessSpecial(PROCESS_SPECIAL_ADD_TABLE_ITEM_TO_STORAGE, 0, 0) ) { }
    }
    end;
}